{
	"name": "pug-compiler-explorer",
	"version": "0.0.1",
	"description": "A web tool to explore the results generated by pug compiler.",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"open-site": "npx http-server ./build -o",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "svelte-kit sync && eslint . && prettier --plugin-search-dir . --check .",
		"format": "svelte-kit sync && eslint . --fix && prettier --plugin-search-dir . --write ."
	},
	"dependencies": {
		"@ota-meshi/site-kit-monaco-editor-svelte": "^0.1.0",
		"assert": "^2.0.0",
		"pug-code-gen": "^3.0.2",
		"pug-lexer": "^5.0.1",
		"pug-parser": "^6.0.0",
		"pug-runtime": "^3.0.1"
	},
	"devDependencies": {
		"@ota-meshi/eslint-plugin": "^0.15.0",
		"@sveltejs/kit": "^2.0.0",
		"@types/node": "^20.0.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.27.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-json-schema-validator": "^4.0.2",
		"eslint-plugin-jsonc": "^2.5.0",
		"eslint-plugin-markdown": "^3.0.0",
		"eslint-plugin-node-dependencies": "^0.11.0",
		"eslint-plugin-prettier": "^5.0.0",
		"eslint-plugin-svelte": "^2.13.0",
		"eslint-plugin-yml": "^1.2.0",
		"monaco-editor": "^0.46.0",
		"prettier": "^3.0.0",
		"prettier-plugin-svelte": "^3.0.0",
		"svelte": "^4.0.0",
		"svelte-adapter-ghpages": "^0.2.0",
		"svelte-check": "^3.0.0",
		"svelte-preprocess": "^5.0.0",
		"tslib": "^2.3.1",
		"typescript": "^5.0.0",
		"typescript-eslint-parser-for-extra-files": "^0.6.0",
		"vite": "^5.0.0"
	},
	"type": "module",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ota-meshi/pug-compiler-explorer.git"
	},
	"keywords": [
		"pug",
		"pugjs"
	],
	"author": "Yosuke Ota (https://github.com/ota-meshi)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ota-meshi/pug-compiler-explorer/issues"
	},
	"homepage": "https://github.com/ota-meshi/pug-compiler-explorer#readme"
}
